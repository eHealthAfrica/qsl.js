{
  "name": "qsl",
  "version": "1.0.2",
  "description": "QSL parser",
  "main": "index.js",
  "scripts": {
    "prebuild": "mkdir -p dist",
    "build": "browserify index.js -s qsl -o dist/qsl.js",
    "postbuild": "uglifyjs dist/qsl.js -mc > dist/qsl.min.js",
    "test": "mocha",
    "watch": "mocha --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eHealthAfrica/qsl.js.git"
  },
  "keywords": [
    "qsl",
    "parser",
    "questionnaire"
  ],
